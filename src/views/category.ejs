                <script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
                <script src="/js/footer.js" type="text/javascript"></script>
                <script src="/js/ads.js" type="text/javascript"></script>
                <script>

                  $('.notification').hide();
                  function openNotification() {
                    $('.notification').toggle();
                  }
                  $('.clear-notification').click(function () {
                    $('.notification').hide();
                  });
                  $('.articlelink').click(function () {
                    var qualify = $(this).attr('qualify');
                    if (!window.showAds) {
                      $('#adPopup').css('display', 'block');
                      $('.popup-back').css('display', 'block');
                    } else {
                      if (qualify == "true") {
                        $('#qualifyPopup').css('display', 'block');
                        $('.popup-back').css('display', 'block');
                      } else {
                        // if ($(this).attr('url') != "#") {
                        window.open($(this).attr('url'));
                        // }
                      }
                    }
                  });
                  $('.popup-back').click(function () {
                    $(".popup").css('display', 'none');
                    $(".popup-back").css('display', 'none');
                  });

                  $('.closepopup').click(function () {
                    $(".popup").css('display', 'none');
                    $(".popup-back").css('display', 'none');
                  });
                  $(function () {
                    $('#q').on('input', function () {
                      var value = $(this).val();
                      $.ajax({
                        url: "/suggestion",
                        type: 'post',
                        data: { term: value },
                        success: function (data) {
                          var template = "";
                          for (i = 0; i < data.data.length; i++) {
                            var _template = '<li class="list-group-item">' + data.data[i] + '</li>';
                            template = template + _template;
                          }
                          $('#suggestion').empty();
                          $('#suggestion').append(template);
                          $('#suggestion').show();
                          $('.list-group-item').click(function () {
                            $('#q').val($(this).text());
                            $('#suggestion').hide();
                          })
                        }
                      })
                    });
                  });
                  var request_flag = true;
                  var page = parseInt($('#page').val());
                  var scrollTop = $(window).scrollTop();
                  var TotalHeight = $(window).height();
                  var slug = "<%= category.slug%>";
                  var scrollPercentage = scrollTop / TotalHeight;
                  $(window).scroll(function () {
                    page++;
                    var total = parseInt($('#total').val());
                    if (scrollPercentage > 0.7) {
                      if (request_flag == true) {
                        $.ajax({
                          url: "/kategory-ajax",
                          type: 'post',
                          data: { slug: slug, page: page },
                          success: function (data) {
                            $('#page').val(data.page);
                            $('#total').val(data.total);
                            var _data = data.data;
                            if (_data.length < 6) {
                              request_flag = false;
                            }
                            for (var i = 0; i < _data.length; i++) {
                              var showPopup = true;
                              var url = "";
                              if (_data[i].postedBy.roleId == "admin") {
                                url = "/d/" + _data[i].category.slug + "/" + _data[i].slug;
                              } else {
                                url = "/p/" + _data[i].category.slug + "/" + _data[i].slug;
                              }
                              if (_data[i].qualify != "qualify") {
                                showPopup = false;
                              }
                              var user_paid = "<%if(user){%><%= user.paid%><%}%>";
                              if (user_paid == "paid") {
                                if (_data[i].qualify == "qualify") {
                                  showPopup = false;
                                }
                              }
                              var template = '<div class="col-md-4">' +
                                '<div class="recommnet">' +
                                '<div class="recommnet-img">' +
                                '<img src="' + _data[i].file + '">' +
                                '</div>' +
                                '<div class="recommnet-cate">' +
                                '<a href="javascript:void(0);"style="font-family: Montserrat;"><span>' + _data[i].category.name + '</span></a></div>' +
                                '<div class="recommnet-heading2">' +
                                '<p style="overflow-wrap: break-word;font-size: 18px;">' +
                                '<a url="' + url + '" type="button" class="articlelink" qualify="' + showPopup + '" style="color:black;font-family: EB Garamond;font-size: 1.2em;">' + _data[i].title.charAt(0).toUpperCase() + _data[i].title.substring(1) + ' </a></p>' +
                                '<p style="overflow-wrap: break-word; font-family: EB Garamond;">' +
                                _data[i].summary + '</p> </div></div></div>';
                              $('#article_list').append(template);
                            }
                            $('.articlelink').click(function () {
                              var qualify = $(this).attr('qualify');
                              if (!window.showAds) {
                                $('#adPopup').css('display', 'block');
                                $('.popup-back').css('display', 'block');
                              } else {
                                if (qualify == "true") {
                                  $('#qualifyPopup').css('display', 'block');
                                  $('.popup-back').css('display', 'block');
                                } else {
                                  // if ($(this).attr('url') != "#") {
                                  window.open($(this).attr('url'));
                                  // }
                                }
                              }
                            });
                          }
                        })
                      }
                    }
                  })
                </script>
</body>

