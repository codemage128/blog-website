                              <%
                                var showPopup = true;
                                var url = "";
                                if(post[i].postedBy.roleId == "admin"){
                                  url = "/d/" + post[i].category.slug + "/" + post[i].slug;
                                }else{
                                  url = "/p/" + post[i].category.slug + "/" + post[i].slug;
                                }
                                if(post[i].qualify != "qualify"){
                                  showPopup = false;
                                }
                                if(user){
                                  if(user.paid == "paid"){
                                    if(post[i].qualify == "qualify"){
                                      showPopup = false;
                                    }
                                  }
                                }
                              %>
                              <a url="<%= url%>" type="button" class="articlelink" qualify="<%= showPopup%>"
